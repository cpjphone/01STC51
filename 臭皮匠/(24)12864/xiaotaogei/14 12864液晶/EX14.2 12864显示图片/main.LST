 220          void picshow(uchar code *bmp)
 221          { 
 222   1       uchar i,j,k,up_down;
 223   1       up_down=0x80;             //先写上半屏
 224   1       wcode(0x34);              //设置图形模式
 225   1       wcode(0x36);              //设置图形模式
 226   1      for(k=0;k<2;k++)           //分上下半屏先后写入,所以循环2次，i为0时写上半屏，i为1时写下半屏
 227   1        {
 228   2         for(i=0;i<32;i++)       //上下半屏各32行，共64行     
 229   2          { 
 230   3            wcode(0x80 + i);     //设置垂直地址
 231   3            wcode(up_down);      //设置水平地址
 232   3            for(j=0;j<16;j++)    //写16个字节，16*8=128竖
 233   3             {
 234   4               wdata(*bmp);      //写入一字节
 235   4               bmp++;            //加1
 236   4             }
 237   3          }
 238   2          up_down=0x88;           //准备写下半屏
 239   2        }           
 240   1      }
